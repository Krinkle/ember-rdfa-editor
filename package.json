{
  "name": "@lblod/ember-rdfa-editor",
  "version": "0.44.0",
  "description": "Ember addon wrapping an RDFa editor with a public API",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lblod/ember-rdfa-editor.git"
  },
  "license": "MIT",
  "author": "redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:errors": "eslint . --quiet",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "icons": "svg-symbols ./public/icons > ./public/say-symbolset.svg",
    "prepare": "svg-symbols ./public/icons > ./public/say-symbolset.svg",
    "release": "release-it"
  },
  "dependencies": {
    "@ember/optional-features": "^1.3.0",
    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/tracking": "^1.0.0",
    "@lblod/marawa": "^0.8.0-beta.2",
    "@types/diff-match-patch": "^1.0.32",
    "common-tags": "^1.8.0",
    "diff-match-patch": "^1.0.5",
    "dompurify": "^2.0.12",
    "ember-auto-import": "^1.6.0",
    "ember-classic-decorator": "^1.0.3",
    "ember-cli-babel": "^7.21.0",
    "ember-cli-htmlbars": "^5.2.0",
    "ember-cli-typescript": "^3.1.3",
    "ember-concurrency": "^1.0.0",
    "ember-concurrency-decorators": "^1.0.0",
    "ember-concurrency-ts": "^0.2.0",
    "ember-decorators": "^6.1.1",
    "ember-feature-flags": "^6.0.0",
    "ember-focus-trap": "^0.5.0",
    "ember-truth-helpers": "^2.0.0",
    "mdn-polyfills": "^5.16.0",
    "ember-intl": "^5.6.2"
  },
  "devDependencies": {
    "@glimmer/component": "^1.0.1",
    "@types/common-tags": "^1.8.0",
    "@types/ember": "^3.1.1",
    "@types/ember-qunit": "^3.4.7",
    "@types/ember__test-helpers": "^0.7.9",
    "@types/qunit": "^2.9.0",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.20.2",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-autoprefixer": "^1.0.0",
    "ember-cli-code-coverage": "^1.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-cli-update": "^0.45.0",
    "ember-cli-yuidoc": "^0.9.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.20.2",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.9.1",
    "ember-test-selectors": "^5.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.5.0",
    "eslint-plugin-ember": "^8.9.1",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.2.0",
    "release-it": "^14.6.1",
    "sass": "^1.26.3",
    "svg-symbols": "^1.0.5",
    "typescript": "^3.9.1-rc",
    "yuidoc-ember-theme": "^2.0.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "paths": [
      "lib/plugin-system-dispatcher"
    ]
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 1 Edge version"
  ],
  "release-it": {
    "npm": {
      "publish": false
    }
  }
}
