<AuContent @skin="tiny">
  <AuToolbar as |Group|>
    <Group>
      <AuHeading @level="5" @skin="5">Backlinks</AuHeading>
    </Group>
    <Group>
      <AuButton @icon="plus" @skin="naked">
        Add backlink
      </AuButton>
    </Group>
  </AuToolbar>
  <AuList @divider={{true}} as |Item|>
    {{#each-in @node.value.attrs.backlinks as |pred prop|}}
      <Item>
        <AuButton
          @icon="external-link"
          @skin="link"
          {{on "click" (fn this.goToNodeWithId prop.nodeId)}}
        >{{pred}}</AuButton>
      </Item>
    {{/each-in}}
  </AuList>
</AuContent>